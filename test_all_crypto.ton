// Kompletny test wszystkich funkcji kryptograficznych
print("=== Test wszystkich funkcji kryptograficznych ===");

// Test SHA-256
print("SHA-256 hash of 'test':");
print(sha256_hash("test"));

// Test MD5
print("MD5 hash of 'test':");
print(md5_hash("test"));

// Test Base64 encoding/decoding
print("Base64 encode 'hello':");
var encoded = base64_encode_text("hello");
print(encoded);
print("Base64 decode:");
print(base64_decode_text(encoded));

// Test random functions
print("Random int (0-100):");
print(random_int(0, 100));
print("Random string (length 10):");
print(random_string(10));

// Test XOR
print("XOR 'hello' with 'world':");
print(xor_strings("hello", "world"));

// Test Caesar cipher
print("Caesar encrypt 'hello' with shift 3:");
var caesar_encrypted = caesar_encrypt("hello", 3);
print(caesar_encrypted);
print("Caesar decrypt:");
print(caesar_decrypt(caesar_encrypted, 3));

// Test ROT13
print("ROT13 'hello':");
print(rot13("hello"));

// Test character functions
print("Character code of 'A':");
print(char_code("A"));
print("Character from code 65:");
print(char_from_code(65));

