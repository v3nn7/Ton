// Test file for crypto functions
// This tests the embedded cryptographic functions

print("Testing crypto functions...");

// Test basic hash functions
let test_text = "Hello World";
print("Test text: " + test_text);

// Test SHA-256
let sha_result = sha256_hash(test_text);
print("SHA-256: " + sha_result);

// Test MD5
let md5_result = md5_hash(test_text);
print("MD5: " + md5_result);

// Test Base64 encoding/decoding
let encoded = base64_encode_text(test_text);
print("Base64 encoded: " + encoded);
let decoded = base64_decode_text(encoded);
print("Base64 decoded: " + decoded);

// Test random functions
let rand_num = random_int(1, 100);
print("Random number (1-100): " + rand_num);

let rand_str = random_string(8);
print("Random string (8 chars): " + rand_str);

// Test XOR
let xor_result = xor_strings("ABC", "XYZ");
print("XOR('ABC', 'XYZ'): " + xor_result);

// Test Caesar cipher
let caesar_encrypted = caesar_encrypt(test_text, 3);
print("Caesar encrypted (+3): " + caesar_encrypted);
let caesar_decrypted = caesar_decrypt(caesar_encrypted, 3);
print("Caesar decrypted: " + caesar_decrypted);

// Test ROT13
let rot13_result = rot13(test_text);
print("ROT13: " + rot13_result);

print("All tests completed!");